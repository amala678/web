<!DOCTYPE html>
<html>
<head>
  <title>Display Students</title>
</head>
<body>
  <h1>List of Students</h1>
  <ul>
    <% studentsList.forEach(function(student) { %>
      <li><%= student.name %> - Age: <%= student.age %>, Grade: <%= student.grade %></li>
    <% }) %>
  </ul>
</body>
</html>
